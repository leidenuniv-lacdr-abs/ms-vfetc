<tool id="ms-vfetc" name="ms-vfetc" version="0.1.0">
<stdio>
    <exit_code range="1:" />
</stdio>
<command><![CDATA[
php -f src/vfetc.php files="$input1" outputfile="$output1"
]]></command>
<inputs>
    <param type="data" name="input1" format="tabular" label="Zip with exported batches"/>
</inputs>
<outputs>
    <data name="output1" format="tabular" label="Generic features file"/>
</outputs>
<help><![CDATA[
Converts MS Vendor export formats to a generic format for data analysis
]]></help>
</tool>